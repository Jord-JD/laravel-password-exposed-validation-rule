{
  "name": "jord-jd/laravel-password-exposed-validation-rule",
  "description": "Laravel validation rule that checks if a password has been exposed in a data breach",
  "type": "library",
  "require": {
    "php": "^7.4||^8.0",
    "jord-jd/password_exposed": "^5.0",
    "illuminate/contracts": "^8.0||^9.0||^10.0||^11.0||^12.0"
  },
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Jordan Hall",
      "email": "jordan@hall05.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "JordJD\\LaravelPasswordExposedValidationRule\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "replace": {
    "divineomega/laravel-password-exposed-validation-rule": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0-dev"
    }
  }
}
